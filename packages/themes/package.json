{
    "name": "@amazigh-ui/themes",
    "version": "0.0.1",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "license": "MIT",
    "files": [
      "dist/**"
    ],
    "scripts": {
      "build": "tsup src/index.ts --format esm,cjs --dts",
      "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
      "lint": "eslint \"src/**/*.ts*\"",
      "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
    },
    "devDependencies": {
      "tsup": "^6.7.0",
      "typescript": "^5.0.4"
    },
    "dependencies": {
      "@amazigh-ui/core": "workspace:*"
    },
    "publishConfig": {
      "access": "public"
    }
  }
